def environmentFileName = 'para.groovy'
// def sharedServiceAWSProfileName = 'agileedgedocker'
pipeline {

    agent any

    stages {
        stage('Clean Workspace') {
            steps { 
                cleanWs()
            }
        }

       stage('Checkout Source') {
            steps {
               sh 'echo ${env.name}'
               sh 'echo ${env.git_cr}'
               
               
               
               
               load "./${environmentFileName}"
               
      }
    }

    }

}
